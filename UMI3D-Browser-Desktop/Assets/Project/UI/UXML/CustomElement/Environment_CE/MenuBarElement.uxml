                <ui:VisualElement style="background-image: url(&apos;/Assets/Project/UI/UI_connection/menu_bar/separateur.png&apos;); width: 20px; -unity-background-scale-mode: scale-to-fit; align-items: center; justify-content: center; height: 50px;" />
