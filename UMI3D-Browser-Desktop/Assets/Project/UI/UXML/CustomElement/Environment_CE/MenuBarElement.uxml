    <Style src="/Assets/Project/UI/UXML/menu-bar.uss" />
    <BrowserDesktop.UI.CustomElement.MenuBarElement style="height: auto; background-color: rgb(63, 63, 66); flex-grow: 0; flex-direction: column; flex-shrink: 0; width: 100%; justify-content: space-between; align-items: center;">
